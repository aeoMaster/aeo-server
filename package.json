{
  "name": "ai-content-api-server",
  "version": "1.0.0",
  "description": "Backend server for AI Content Analyzer with TypeScript",
  "engines": {
    "node": ">=20.0.0",
    "npm": ">=9.0.0"
  },
  "main": "dist/index.js",
  "scripts": {
    "start": "node dist/index.js",
    "dev": "ts-node-dev --respawn --transpile-only src/index.ts",
    "build": "tsc",
    "test": "jest --config jest.config.js",
    "lint": "eslint . --ext .ts",
    "format": "prettier --write \"src/**/*.ts\"",
    "test:health": "curl -s http://localhost:5000/health | jq .",
    "test:health-all": "./scripts/test-health.sh",
    "deploy:local": "./scripts/deploy.sh",
    "setup:aws": "./scripts/complete-setup.sh",
    "validate:env": "node scripts/validate-env.js"
  },
  "dependencies": {
    "@mozilla/readability": "^0.6.0",
    "@sendgrid/mail": "^8.1.5",
    "@types/axios": "^0.14.4",
    "@types/stripe": "^8.0.416",
    "axios": "^1.9.0",
    "bcryptjs": "^2.4.3",
    "cheerio": "^1.1.0",
    "chrome-launcher": "^1.2.0",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "jsdom": "^26.1.0",
    "jsonwebtoken": "^9.0.2",
    "langchain": "^0.3.27",
    "lighthouse": "^12.6.0",
    "mongoose": "^8.0.3",
    "openai": "^4.28.0",
    "passport": "^0.7.0",
    "passport-google-oauth20": "^2.0.0",
    "passport-jwt": "^4.0.1",
    "stripe": "^18.1.1",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@babel/generator": "^7.23.6",
    "@babel/traverse": "^7.23.6",
    "@babel/types": "^7.23.6",
    "@types/bcryptjs": "^2.4.6",
    "@types/cheerio": "^0.22.35",
    "@types/cors": "^2.8.17",
    "@types/dotenv": "^8.2.0",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.14",
    "@types/jsdom": "^21.1.7",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/node": "^20.10.5",
    "@types/passport": "^1.0.16",
    "@types/passport-google-oauth20": "^2.0.14",
    "@types/passport-jwt": "^4.0.1",
    "@types/supertest": "^3.0.3",
    "@typescript-eslint/eslint-plugin": "^6.15.0",
    "@typescript-eslint/parser": "^6.15.0",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "jest": "^29.7.0",
    "prettier": "^3.1.1",
    "supertest": "^7.1.3",
    "ts-jest": "^29.3.2",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.3.3"
  }
}
